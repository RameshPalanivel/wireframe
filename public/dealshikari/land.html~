$(document).ready(function () {
var i = 1;
var delay = 4500;
var interval;
var rotate;
$(window).bind("load", function () {
interval = setInterval(rotate, delay);
$("#img2").attr("src", image2src);
$("#img3").attr("src", image3src);
$("li").click(function () {
clearInterval(interval);
i = $(this).attr("id").substring(2); 
switch (i) {
case "1":
SetOmnitureEvar("evar44", "quad_a_rot_1");
break;
case "2":
