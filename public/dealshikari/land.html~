function () { interval = setInterval(rotate, delay); });
});
rotate = function rotate() {
for (var x = 1; x <= 3; x++) {
var tag = "#image" + x;
var selector = "#li" + x;
var s= "display: ";
var url;
if (x == i) { url = on; s+= "block;"; }
else { url = off; s+= "none;"; }
$(tag).attr({ style: s});
$(selector).css("background-image", "url(" + url+")");
}
i %= 3; i++;
}
});
